#include <iostream>
using namespace std;

int main() {
    int arr[] = {1,1,2,3,1,2,2,3,4,4};
    int n = sizeof(arr) / sizeof(arr[0]);
    bool visited[n];

    for (int i = 0; i < n; i++) {
        visited[i] = false;
    }

    for (int i = 0; i < n; i++) {
        if (visited[i] == true)
            continue;

        int count = 1;
        for (int j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                visited[j] = true;
                count++;
            }
        }
        cout << arr[i] << " - " << count << endl;
    }

    return 0;
}
