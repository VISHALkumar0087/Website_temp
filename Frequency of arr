#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int arr[] = {1,1,2,3,1,2,2,3,4,4};
    int n = sizeof(arr) / sizeof(arr[0]);

    sort(arr, arr + n);

    int count = 1;
    for (int i = 1; i <= n; i++) {
        if (i < n && arr[i] == arr[i - 1]) {
            count++;
        } else {
            cout << arr[i - 1] << " - " << count << endl;
            count = 1;
        }
    }

    return 0;
}
